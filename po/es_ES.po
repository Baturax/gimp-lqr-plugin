#: ../src/interface.c:225
msgid "GIMP LiquidRescale Plug-In"
msgstr "GIMP LiquidRescale Plug-In"

#: ../src/interface.c:247
msgid "Selected layer"
msgstr "Capa seleccionada"

#. New size
#: ../src/interface.c:309
msgid "Select new width and height"
msgstr "Seleccione nueva anchura y altura"

#: ../src/interface.c:329
msgid "Width:"
msgstr "Anchura:"

#: ../src/interface.c:331
msgid "Height:"
msgstr "Altura:"

#: ../src/interface.c:346
msgid "Choose mode of operation"
msgstr ""

#: ../src/interface.c:354
msgid "Mode:"
msgstr ""

#: ../src/interface.c:359
msgid "LqR only"
msgstr ""

#: ../src/interface.c:360
msgid "LqR + LqR back to the original size"
msgstr ""

#: ../src/interface.c:361
