#. New size
#: ../src/interface.c:324
msgid "Select new width and height"
msgstr "Sélectionner la nouvelle largeur et hauteur"

#: ../src/interface.c:344 ../src/interface_I.c:209
msgid "Width:"
msgstr "Largeur :"

#: ../src/interface.c:346 ../src/interface_I.c:211
msgid "Height:"
msgstr "Hauteur :"

