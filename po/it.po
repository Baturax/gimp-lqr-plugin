#: ../src/interface_I.c:557
#, c-format
msgid "horizontal"
msgstr "orizzontale"

#: ../src/interface_I.c:558
#, c-format
msgid "vertical"
msgstr "verticale"

#: ../src/interface_I.c:559
#, c-format
msgid "Range"
msgstr "Intervallo"

#: ../src/interface_I.c:560
#, c-format
msgid "Next step at"
msgstr "Prossimo passo a"

#: ../src/main.c:175
msgid "scaling which keeps layer features (or removes them)"
msgstr "scala il livello mantenendo i contenuti (o rimuovendoli)"

#: ../src/main.c:179
msgid "Li_quid rescale..."
msgstr "Li_quid rescale..."

