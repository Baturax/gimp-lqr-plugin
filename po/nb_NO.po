#: ../src/interface.c:225
msgid "GIMP LiquidRescale Plug-In"
msgstr "Væske-skalering for GIMP"

#: ../src/interface.c:247
msgid "Selected layer"
msgstr "Valgt lag"

#. New size
#: ../src/interface.c:309
msgid "Select new width and height"
msgstr "Velg ny bredde og høyde"

#: ../src/interface.c:329
msgid "Width:"
msgstr "Bredde:"

#: ../src/interface.c:331
msgid "Height:"
msgstr "Høyde:"

#: ../src/interface.c:346
msgid "Choose mode of operation"
msgstr ""

#: ../src/interface.c:354
msgid "Mode:"
msgstr ""

#: ../src/interface.c:359
msgid "LqR only"
msgstr ""

#: ../src/interface.c:360
msgid "LqR + LqR back to the original size"
msgstr ""

#: ../src/interface.c:361
